                "policies": [
                    "global.isAuthenticated"
                ]
            }
        },
        {
            "method": "PATCH",
            "path": "/invoiceposition/:id/relationships/:relation",
            "handler": "Invoiceposition.updateRelation",
            "config": {
                "policies": [
                    "global.isAuthenticated"
                ]
            }
        },
        {
            "method": "DELETE",
            "path": "/invoiceposition/:id/relationships/:relation",
            "handler": "Invoiceposition.destroyRelation",
            "config": {
                "policies": [
                    "global.isAuthenticated"
                ]
            }
        }
