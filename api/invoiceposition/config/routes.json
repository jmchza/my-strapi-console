{
    "routes": [
        {
            "method": "GET",
            "path": "/invoicepositions",
            "handler": "Invoiceposition.find",
            "config": {
                "policies": [
                    "global.isAuthenticated"
                ]
            }
        },
        {
            "method": "GET",
            "path": "/invoiceposition/:id",
            "handler": "Invoiceposition.findOne",
            "config": {
                "policies": [
                    "global.isAuthenticated"
                ]
            }
        },
        {
            "method": "GET",
            "path": "/invoiceposition/:id/relationships/:relation",
            "handler": "Invoiceposition.findOne",
            "config": {
                "policies": [
                    "global.isAuthenticated"
                ]
            }
        },
        {
            "method": "GET",
            "path": "/invoiceposition/:id/:relation",
            "handler": "Invoiceposition.findOne",
            "config": {
                "policies": [
                    "global.isAuthenticated"
                ]
            }
        },
        {
            "method": "POST",
            "path": "/invoiceposition",
            "handler": "Invoiceposition.create",
            "config": {
                "policies": [
